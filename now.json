{
    "version": 2,
    "alias": "nextjs-project",
    "name": "nextjs2-project-temp",
    "builds": [
      { "src": "package.json", "use": "@now/next" },
      { "src": "/static/*", "use": "@now/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/post/(?<id>[^/]+)$", "dest": "/post?id=$id" }
    ]
  }